.container {
  width: 100%;
  height: 130rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding: 0rpx 30rpx 0rpx 30rpx;
  border-radius: 12rpx;
  border-style: solid;
  box-shadow: 0px 1px 4px 1px #DDDDDD;
  border-width: 0rpx;
  margin-bottom: 30rpx;
}

.bottom-info {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin-bottom: 15rpx;
}
.title {
  margin-top: 20rpx;
}
.container text{
  font-size: 24rpx;
  color: #333;
}